{
    "manifest_version": 3,
    "name": "Clock",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["https://*.hh.ru/vacancy/*"],
            "js": ["script.js"],
            "css": ["style.css"],
            "run_at": "document_start"
        }
    ],
    "permissions": [ "storage", "scripting", "webNavigation" ]
}